{
  "name": "@gruhn/regex-utils",
  "description": "TypeScript library for regex equivalence, intersection, complement and other utilities that go beyond string matching.",
  "author": "Niklas Gruhn",
  "license": "MIT",
  "repository": "github:gruhn/regex-utils",
  "version": "0.0.0-development",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "bench": "npm run build && 0x --output-dir prof",
    "build": "tsc --project tsconfig.build.json && tsc-alias",
    "build:docs": "npm run build && typedoc --tsconfig tsconfig.build.json && cp -r dist/ docs/dist/ && cp ./*.html docs/",
    "serve:docs": "http-server -p 8080 -o docs/"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "0x": "^5.8.0",
    "fast-check": "^3.23.2",
    "http-server": "^14.1.1",
    "semantic-release": "^24.2.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.15",
    "typedoc": "^0.28.4",
    "typescript": "^5.8.3",
    "vitest": "^3.0.5"
  }
}
